class Month
  def self.all
    unless @months
      @months = []
      first_month = Employee.all.order("created_at ASC").first.created_at.beginning_of_month
      month = first_month
      while month <= Date.today.+(1.month)
        @months << month
        month += 1.month
      end
    end
    puts @months
    @months
  end
end
